{
  "name": "ide-haskell",
  "main": "./lib/ide-haskell",
  "version": "1.9.6",
  "description": "Haskell IDE",
  "keywords": [
    "ide-haskell",
    "ide",
    "haskell"
  ],
  "repository": "https://github.com/atom-haskell/ide-haskell",
  "license": "MIT",
  "activationHooks": [
    "language-haskell:grammar-used"
  ],
  "engines": {
    "atom": ">=1.17.0 <2.0.0"
  },
  "providedServices": {
    "ide-haskell-upi": {
      "description": "Universal pluggable interface",
      "versions": {
        "0.0.1": "provideUpi",
        "0.2.0": "provideUpi",
        "0.3.0": "provideUpi3"
      }
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "1.0.0": "consumeLinter"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "ide-haskell-upi-plugin": {
      "versions": {
        "^0.3.0": "consumeUpi3"
      }
    }
  },
  "dependencies": {
    "atom-haskell-utils": "^1.0.0",
    "atom-highlight": "^0.3.0",
    "atom-select-list": "^0.2.0",
    "etch": "^0.12.5",
    "temp": "^0.8.1"
  },
  "devDependencies": {
    "@types/node": "^8.0.14",
    "@types/temp": "^0.8.29",
    "tslint": "^5.5.0",
    "typedoc": "^0.7.1",
    "typescript": "^2.4.0"
  }
}
